    "font-awesome": "*",
    "ie-shim": "*",
    "jquery": "*",
    "metismenu": "*",
    "ng2-bootstrap": "^1.1.16-11",
    "ng2-charts": "*",
    "pace": "*",
    "pace-progress": "*",
    "reflect-metadata": "*",
    "rxjs": "*",
