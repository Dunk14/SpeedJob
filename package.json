{
  "name": "inspinia_angular2_starter",
  "version": "1.0.0",
  "description": "Inspinia Admin Theme",
  "repository": "https://wrapbootstrap.com/theme/inspinia-responsive-admin-theme-WB0R5L90S",
  "scripts": {
